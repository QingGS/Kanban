friend(uer_id,friend_id)


bag_player(user_id,player_id,score,salary,input_data_id,duedate)eg:salary=600(pay monthly)
input_data(id)
bag_equip(user_id,equip_id,num)
bag_piece(user_id,piece_id,num)
bag_prop(user_id,fund_card_num,exp_card_num)
bag_trail_card(user_id,player_id,num,time)
prop_using(user_id,type,duetime)//type:fund-0,exp-1


team_info(id,name,city,intro)
player_base(id,birthday,country,height,weight,armspan,reach_height,draft,contract,season_id,team_id,cloth_num,pos1,pos2,price,score)eg:price=5000(recruit)
